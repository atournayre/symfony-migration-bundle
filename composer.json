{
    "description": "Atournayre Symfony Migration.",
    "name": "atournayre/symfony-migration-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["generator", "code generator", "scaffolding", "scaffold"],
    "authors": [
        {
            "name": "AurÃ©lien Tournayre",
            "email": "aurelien.tournayre@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1.1",
        "doctrine/common": "3.2.1",
        "symfony/maker-bundle": "^1.36",
        "symfony/yaml": "5.4.x-dev"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "6.1.x-dev"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Atournayre\\Bundle\\SymfonyMigrationBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Atournayre\\Bundle\\SymfonyMigrationBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
